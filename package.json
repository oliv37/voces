{
  "name": "voces",
  "version": "1.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --base-href /voces/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "predeploy": "npm run lint && npm run build:prod",
    "deploy": "gh-pages -d dist/voces/browser -m 'ci: deploy site'",
    "serve:ssr:voces": "node dist/voces/server/server.mjs",
    "update": "ng update @angular/core @angular/cli"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.5",
    "@angular/compiler": "^21.0.5",
    "@angular/core": "^21.0.5",
    "@angular/forms": "^21.0.5",
    "@angular/platform-browser": "^21.0.5",
    "@angular/platform-browser-dynamic": "^21.0.5",
    "@angular/platform-server": "^21.0.5",
    "@angular/router": "^21.0.5",
    "@angular/service-worker": "^21.0.5",
    "@angular/ssr": "^21.0.3",
    "express": "^5.1.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.5",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "angular-eslint": "^21.1.0",
    "eslint": "^9.39.1",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.48.1",
    "vitest": "^4.0.14"
  }
}
